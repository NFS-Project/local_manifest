<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Remote -->
  <remote name="NFS"
          fetch="https://github.com/NFS-project" />

  <remote name="NFSclang"
          fetch="https://pf.osdn.net/gitroot/n/nf/nfs86/"
          revision="master" />


  <!-- Toolchain -->
  <project path="prebuilts/clang/host/linux-x86/clang-NFS" name="NFS-clang" remote="NFSclang" clone-depth="1"/>

  <!-- hardware/interfaces -->
  <remove-project name="hardware_interfaces" />
  <project path="hardware/interfaces" name="android_hardware_interfaces" remote="NFS" revision="thirteen" />

  <!-- system/libhidl -->
  <remove-project name="system_libhidl" />
  <project path="system/libhidl" name="android_system_libhidl" remote="NFS" revision="arrow-13.0" />

  <!-- libbfqio -->
  <project path="device/xiaomi/rosy/libbfqio" name="android_system_libbfqio" remote="NFS" clone-depth="1" revision="master" />

  <!-- misc -->
  <remove-project name="android_hardware_lineage_compat" />

</manifest>
